\pset border 2
\pset unicode_header_linestyle double

\set HISTSIZE 99999
\set HISTCONTROL ignoreboth
\set OM_PROMPT1_DEF :PROMPT1
\set OM_PROMPT2_DEF :PROMPT2
\set OM_PROMPT3_DEF :PROMPT3

\i :OM_INSTALL_DIR/utilmake.sql
:pro
